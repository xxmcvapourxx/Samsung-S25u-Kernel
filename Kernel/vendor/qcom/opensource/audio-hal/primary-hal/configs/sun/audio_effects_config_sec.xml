<!-- } SEC_AUDIO_EFFECT_AIDL -->
    </effects>

    <preprocess>
        <stream type="voice_communication">
            <apply effect="aec"/>
        </stream>
    </preprocess>
<!--  {  SEC_AUDIO_SUPPORT_AIDL_EFFECT  -->
    <postprocess>
        <stream type="music">
            <apply effect="sa3d"/>
        </stream>
        <stream type="ring">
            <apply effect="sa3d"/>
        </stream>
        <stream type="alarm">
            <apply effect="sa3d"/>
        </stream>
    </postprocess>
<!--  }  SEC_AUDIO_SUPPORT_AIDL_EFFECT  -->
</audio_effects_conf>

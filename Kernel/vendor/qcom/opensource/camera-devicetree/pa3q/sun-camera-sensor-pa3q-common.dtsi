		cam_vio-supply = <&s2mpb03_0_l3>;				/* VDD_TELE5X_IO_1P8 */
		cam_vana-supply = <&s2mpb02_l9>;				/* VDD_TELE5X_A1P8 */
		cam_v_custom2-supply = <&BOB1>;     	/* VREG_BOB1_3P5 */
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;

		regulator-names = "cam_vdig", "cam_vio", "cam_vana", "cam_clk", "cam_v_custom2";
		rgltr-cntrl-support;
		rgltr-min-voltage = <843750 1800000 1800000 0 3008000>;
		rgltr-max-voltage = <843750 1800000 1800000 0 4000000>;
		rgltr-load-current = <200000 200000 200000 0 700000>;

		gpio-no-mux = <0>;

		pinctrl-names = "cam_default", "cam_suspend", "cam_vana";
		pinctrl-0 = <&cam_sensor_mclk4_active &tele5xcam_rst_n_active &tele5xcam_ldo_en_active>;
		pinctrl-1 = <&cam_sensor_mclk4_suspend &tele5xcam_rst_n_suspend &tele5xcam_ldo_en_suspend>;

		gpios = <&tlmm 93 0>, <&pm8550ve_g_gpios 4 0>, <&pm8550ve_g_gpios 5 0>, <&tlmm 94 0>;
		gpio-reset = <1>;
		gpio-custom1 = <2>;                 		/* VDD_TELE5X_A2P8 */
		gpio-req-tbl-num = <0 1 2 3>;
		gpio-req-tbl-flags = <1 0 0 1>;
		gpio-req-tbl-label = "CAMIF_MCLK4", "CAM_RESET4", "TELE5X_LDO_EN", "CAMIF_MCLK5";

		sensor-mode = <0>;
		cci-master = <CCI_MASTER_1>;

		clocks = <&cambistmclkcc CAM_BIST_MCLK_CC_MCLK4_CLK>, <&cambistmclkcc CAM_BIST_MCLK_CC_MCLK5_CLK>;
		clock-names = "cam_clk", "flicker_mclk5";
		clock-cntl-level = "turbo";
		clock-rates = <19200000 19200000>;

		cam,isp = <0>;                  // 0 : INT , 1 : EXT , 2 : SOC
		cam,cal_memory = <2>;           // 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP
		cam,read_version = <0>;         // 0 : SYSFS , 1 : CAMON
		cam,core_voltage = <0>;         // 0 : N , 1 : Y
		cam,upgrade = <0>;              // 0 : N , 1 : SYSFS , 2 : CAMON
		cam,fw_write = <0>;             // 0 : N , 1 : OS , 2 : SD , 3 : ALL
		cam,fw_dump = <0>;              // 0 : N , 1 : Y

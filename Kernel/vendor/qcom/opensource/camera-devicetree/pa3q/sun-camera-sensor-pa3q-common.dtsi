		regulator-names = "cam_vaf", "cam_vio";
		rgltr-cntrl-support;
		rgltr-min-voltage = <3300000 1800000>;
		rgltr-max-voltage = <3300000 1800000>;
		rgltr-load-current = <10000 10000>;

		qcom,cam-power-seq-type = "cam_vio", "cam_vaf";
		qcom,cam-power-seq-cfg-val = <1 1>;
		qcom,cam-power-seq-delay = <1 12>;

		cci-master = <CCI_MASTER_1>;
	};
	eeprom_rear_tele_x5: qcom,eeprom6 {
		status = "ok";

		cell-index = <6>;
		compatible = "qcom,eeprom";
		reg = <0x54>;
		slave-addr = <0xA8>;

		cam_vio-supply = <&s2mpb02_l8>;			/* VDD_WIDECAM_IO_1P8 */
		cam_vaf-supply = <&s2mpb02_l10>;			/* VDD_TELE5X_AF_OIS_IO_1P8 */

		regulator-names = "cam_vio", "cam_vaf";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 1800000>;
		rgltr-max-voltage = <1800000 1800000>;
		rgltr-load-current = <120000 120000>;
		gpio-no-mux = <0>;

		cci-master = <CCI_MASTER_1>;
	};

};
